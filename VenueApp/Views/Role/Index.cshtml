@model List<VenueApp.Models.UserType>

@{
    ViewBag.Title = " List of User Roles";
}

<h1>@ViewBag.Title</h1>

<ul class="list-group">

    @foreach (var role in Model)
    {
        <li class="list-group-item">ID = @role.ID: <b>@role.Name</b> &nbsp; @(role.Protected ? "(Protected)" : "")</li>
    }
</ul>


@if (Model.Count == 0)
{
    <p>No User Types or Roles yet!</p>
}
   